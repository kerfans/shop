                 var deliver_type=true;
                 for(var i=0;i<$(".deliver_type").length;i++){
                     if($(".deliver_type:eq("+i+")").attr("checked")){
                         deliver_type=false;
                         break;
                     }
                 }
                 if(deliver_type){
                     alert('请选择配送类型');
                     return false;
                 }
                 $("button").css({ "color": "white" }).attr({ "disabled": "disabled" }).html('禁用');
                 $("#states"+i).css({ "color": "red" }).attr({ "disabled": "disabled" }).html('处理中...');
                 var string="<input type='hidden' name='states' value='1'/>";
                 $("#states").html(string);
                 $("form").submit();
             }
             if(i==5){
                 var deliver_type=true;
                 for(var i=0;i<$(".deliver_type").length;i++){
                     if($(".deliver_type:eq("+i+")").attr("checked")){
                         deliver_type=false;
                         break;
                     }
                 }
                 if(deliver_type){
                     alert('请选择配送类型');
                     return false;
                 }
                 $("button").css({ "color": "white" }).attr({ "disabled": "disabled" }).html('禁用');
                 $("#states"+i).css({ "color": "red" }).attr({ "disabled": "disabled" }).html('处理中...');
                 var string="<input type='hidden' name='states' value='5'/>";
                 $("#states").html(string);
                 $("form").submit();
             }
             if(i==6){
                 $("button").css({ "color": "white" }).attr({ "disabled": "disabled" }).html('禁用');
                 $("#states"+i).css({ "color": "red" }).attr({ "disabled": "disabled" }).html('处理中...');
                 window.location.href="<?php echo SITE; ?>mendian/update_states/"+order_id+"/6/"+encodeURI(caozuo);
             }
             if(i==4){
                 if(confirm('您确定要作废么?')){
                     $("button").css({ "color": "white" }).attr({ "disabled": "disabled" }).html('禁用');
                     $("#states"+i).css({ "color": "red" }).attr({ "disabled": "disabled" }).html('处理中...');
                     window.location.href="<?php echo SITE; ?>mendian/del/"+order_id+"/4/"+encodeURI(caozuo);
                 }
             }
             if(i==8){
                 $("button").css({ "color": "white" }).attr({ "disabled": "disabled" }).html('禁用');
                 $("#states"+i).css({ "color": "red" }).attr({ "disabled": "disabled" }).html('处理中...');
                 window.location.href="<?php echo SITE; ?>mendian/update_states/"+order_id+"/8/"+encodeURI(caozuo);
             }
             if(i==111){//返回
                 window.location.href="<?php echo $_SERVER['HTTP_REFERER'];?>";
             }
             if(i==10){
                 $("button").css({ "color": "red" }).attr({ "disabled": "disabled" }).html('禁用');
                 $("#states"+i).css({ "color": "red" }).attr({ "disabled": "disabled" }).html('处理中...');
                 window.location.href="<?php echo SITE; ?>mendian/update_states/"+order_id+"/10/"+encodeURI(caozuo);
             }
