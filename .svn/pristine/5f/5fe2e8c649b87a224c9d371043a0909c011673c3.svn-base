         //状态名称
         function states_name(states){
             var states_name;
             if(states==0){
                 states_name='未沟通';
             }else if(states==1){
                 states_name='已沟通';
             }else if(states==2){
                 states_name='已确认';
             }else if(states==7){
                 states_name='已处理';
             }else if(states==3){
                 states_name='已完成';
             }else if(states==4){
                 states_name='已作废';
             }else if(states==5){
                 states_name='转预存款';
             }else if(states==6){
                 states_name='无法联系';
             }else if(states==9){
                 states_name='2小时违约需求';
             }else if(states==10){
                 states_name='已取消的违约需求';
             }else if(states==11){
                 states_name='经销商缺货退回';
             }else if(states==12){
                 states_name='顾客拒收';
             }
             return states_name;
         }
