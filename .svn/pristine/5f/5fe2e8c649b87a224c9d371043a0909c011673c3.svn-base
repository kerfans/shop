        $(function(){
            $("#get_pro").change(function(){
                var code=$("#get_pro option:selected").val();
                if(code==0){
                    return;
                }
                $.post('<?php echo SITE; ?>mendian/get_city/'+code, function(data){
                    $("#addr").val($("#get_pro option:selected").html());
                    $("#get_city").html(data);
                });
            })
            $("#get_city").change(function(){
                var code=$("#get_city option:selected").val();
                if(code==0){
                    return;
                }
                $.post('<?php echo SITE; ?>mendian/get_dis/'+code, function(data){
                    if(data==0){
                        $("#addr").val($("#get_pro option:selected").html()+$("#get_city option:selected").html()+" ");
                        $("#get_dis").html(data);
                        $("#get_dis").css({ "display": "none" });
                    }else{
                        $("#addr").val($("#get_pro option:selected").html()+$("#get_city option:selected").html());
                        $("#get_dis").css({ "display": "inline-block" })
                        $("#get_dis").html(data);
                    }
                });
                $("#new_address").css({ "display": "inline-block" })
            })
            $("#get_dis").change(function(){
                var code=$("#get_dis option:selected").val();
                if(code==0){
                    return;
                }
                $("#addr").val($("#get_pro option:selected").html()+$("#get_city option:selected").html()+$("#get_dis option:selected").html()+" ");
            })
        })
